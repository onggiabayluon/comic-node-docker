                <info-title class="ellipsis">
                    \{{this.title}}
                </info-title>
            </a>
            <a href="\{{this.chapterUrl}}">
                \{{#each (limitlast this.chapterNameList 1) }}
                    <info-chapter id="chapter-\{{this._id}}">
                    \{{chapterText this.chapter}}
                    </info-chapter>
                \{{/each}}
            </a>
            <info-sub onclick="return subscribe(this);" class="info-sub">
                <input name="comicId" value="temp" type="hidden">
                <i class="fas fa-check"></i>
            </info-sub>
        </bottom-info>
    </div>
    \{{/each}}
    
\{{/if}}
</script>

