        <div id="comic-\{{this.comicId}}" class="glide__slide">
            <glide-img>
                <img width="198" height="160"
                    src="https://s3.eu-central-1.wasabisys.com/cloudimagewallbucket/\{{this.thumbnail}}-thumbnail.webp"
                    alt="">
            </glide-img>
            <bottom-info>
                <a href="">
                    <info-title class="ellipsis">
                        \{{this.title}}
                    </info-title>
                    \{{#each (limitlast this.chapterNameList 1) }}
                        <info-chapter id="chapter-\{{this._id}}">
                        \{{chapterText this.chapter}}
